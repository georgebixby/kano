<h1>Create a test</h1>

<%= form_for (@test) do |f| %>

  <%= f.label :title %><br>
  <%= f.text_field :title %><br>

  <%= f.label :description %><br>
  <%= f.text_area :description %><br>

  <%= f.label :question_one %><br>
  <%= f.text_area :question_one %><br>

  <%= f.label :question_two %><br>
  <%= f.text_area :question_two %><br>

  <%= f.submit %>

<% end %>